# AWS Configuration
AWS_REGION=us-west-2

# RDS Aurora PostgreSQL
RDS_CLUSTER_ARN=arn:aws:rds:us-west-2:023487747239:cluster:hackthon-panel-db
RDS_SECRET_ARN=arn:aws:secretsmanager:us-west-2:023487747239:secret:hackthon-db-secret-xxxxx
RDS_DATABASE=hackthon

# S3 Bucket for Knowledge Base
KB_S3_BUCKET=hackthon-knowledge-base

# Bedrock Knowledge Base
KNOWLEDGE_BASE_ID=XXXXXXXXXXXXX
KB_DATA_SOURCE_ID=YYYYYYYYYYY

# Optional: Browser data collection settings
# (Playwrightブラウザの設定が必要な場合)
BROWSER_HEADLESS=true
